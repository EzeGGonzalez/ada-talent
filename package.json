{
  "name": "ada-talent-app",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Ezequiel Ger√≥nimo Gonzalez <ezequiel@adaitw.com.ar>",
  "private": true,
  "scripts": {
    "pm2:start": "pm2 start build/main.js --name $APP_NAME",
    "pm2:restart": "pm2 stop $APP_NAME && pm2 delete $APP_NAME && npm run pm2:start",
    "pm2:stop": "pm2 stop $APP_NAME && pm2 delete $APP_NAME",
    "pm2": "if pm2 list | grep $APP_NAME; then npm run pm2:restart; else npm run pm2:start; fi",
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "prestart": "npm rebuild node-sassnpm run build",
    "postinstall": "npm rebuild node-sass",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/font-awesome": "^1.0.2",
    "axios": "^0.16.2",
    "body-parser": "^1.18.1",
    "buefy": "^0.5.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.1",
    "crypto": "^1.0.1",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "gm": "^1.23.0",
    "keystone": "^4.0.0-beta.1",
    "mongoose": "^4.11.10",
    "multer": "^1.3.0",
    "nconf": "^0.8.4",
    "nuxt": "^1.0.0-rc3",
    "source-map-support": "^0.4.15"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "backpack-core": "^0.4.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6"
  }
}
